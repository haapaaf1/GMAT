%-----------------------------------------------------------
% This function propagates spacecraft for 8640 seconds
%-----------------------------------------------------------
function [sat, x, y, z, vx, vy, vz] = Propagate8640Secs(sat)

Create Spacecraft sat;
Create Variable x y z vx vy vz;

Propagate Prop(sat) {sat.ElapsedSecs = 8640.0};

% After the Propagate command
x  = sat.X
y  = sat.Y
z  = sat.Z
vx = sat.VX
vy = sat.VY
vz = sat.VZ
