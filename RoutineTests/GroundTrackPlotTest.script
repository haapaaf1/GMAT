Create Spacecraft MMS2;
GMAT MMS2.DateFormat = TAIGregorian;
GMAT MMS2.Epoch = '09 Oct 2004 16:30:00.124';
GMAT MMS2.CoordinateSystem = EarthMJ2000Eq;
GMAT MMS2.DisplayStateType = Keplerian;
GMAT MMS2.SMA = 46999.99999999998;
GMAT MMS2.ECC = 0.7999999999999998;
GMAT MMS2.INC = 28.49999999999999;
GMAT MMS2.RAAN = 90;
GMAT MMS2.AOP = 0;
GMAT MMS2.TA = 180.9999999999999;
GMAT MMS2.DryMass = 100;
GMAT MMS2.Cd = 2;
GMAT MMS2.Cr = 1.4;
GMAT MMS2.DragArea = 1;
GMAT MMS2.SRPArea = 1;
GMAT MMS2.NAIFId = -123456789;
GMAT MMS2.NAIFIdReferenceFrame = -123456789;
GMAT MMS2.Id = 'SatId';
GMAT MMS2.Attitude = CoordinateSystemFixed;
GMAT MMS2.ModelFile = 'C:/Projects/GmatDev/application/data/vehicle/models/aura.3ds';
GMAT MMS2.ModelOffsetX = 0;
GMAT MMS2.ModelOffsetY = 0;
GMAT MMS2.ModelOffsetZ = 0;
GMAT MMS2.ModelRotationX = 0;
GMAT MMS2.ModelRotationY = 0;
GMAT MMS2.ModelRotationZ = 0;
GMAT MMS2.ModelScale = 3;
GMAT MMS2.AttitudeDisplayStateType = 'Quaternion';
GMAT MMS2.AttitudeRateDisplayStateType = 'AngularVelocity';
