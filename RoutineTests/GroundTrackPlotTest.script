GMAT MMS2.EulerAngleSequence = '321';
GMAT MMS2.AngularVelocityX = 0;
GMAT MMS2.AngularVelocityY = 0;
GMAT MMS2.AngularVelocityZ = 0;

Create Spacecraft Sat3;
GMAT Sat3.DateFormat = TAIGregorian;
GMAT Sat3.Epoch = '09 Oct 2004 16:30:00.124';
GMAT Sat3.CoordinateSystem = EarthMJ2000Eq;
GMAT Sat3.DisplayStateType = Keplerian;
GMAT Sat3.SMA = 7999.999999999997;
GMAT Sat3.ECC = 0.01000000000000033;
GMAT Sat3.INC = 28.50000000000003;
GMAT Sat3.RAAN = 90;
GMAT Sat3.AOP = 360;
GMAT Sat3.TA = 181.9999999999999;
GMAT Sat3.DryMass = 100;
GMAT Sat3.Cd = 2;
GMAT Sat3.Cr = 1.4;
GMAT Sat3.DragArea = 1;
GMAT Sat3.SRPArea = 1;
GMAT Sat3.NAIFId = -123456789;
GMAT Sat3.NAIFIdReferenceFrame = -123456789;
GMAT Sat3.Id = 'SatId';
GMAT Sat3.Attitude = CoordinateSystemFixed;
GMAT Sat3.ModelFile = 'C:/Projects/GmatDev/application/data/vehicle/models/aura.3ds';
GMAT Sat3.ModelOffsetX = 0;
GMAT Sat3.ModelOffsetY = 0;
GMAT Sat3.ModelOffsetZ = 0;
GMAT Sat3.ModelRotationX = 0;
GMAT Sat3.ModelRotationY = 0;
GMAT Sat3.ModelRotationZ = 0;
GMAT Sat3.ModelScale = 3;
GMAT Sat3.AttitudeDisplayStateType = 'Quaternion';
GMAT Sat3.AttitudeRateDisplayStateType = 'AngularVelocity';
GMAT Sat3.AttitudeCoordinateSystem = 'EarthMJ2000Eq';
GMAT Sat3.Q1 = 0;
GMAT Sat3.Q2 = 0;
GMAT Sat3.Q3 = 0;
GMAT Sat3.Q4 = 1;
GMAT Sat3.EulerAngleSequence = '321';
GMAT Sat3.AngularVelocityX = 0;
GMAT Sat3.AngularVelocityY = 0;
GMAT Sat3.AngularVelocityZ = 0;

Create GroundStation GroundStation1;

%----------------------------------------
%---------- Formation
%----------------------------------------

%  Create Formation
Create Formation MMS;
GMAT MMS.Add = {MMS1, MMS2};

%----------------------------------------
%---------- ForceModels
%----------------------------------------

Create ForceModel LowEarth_JR;
GMAT LowEarth_JR.CentralBody = Earth;
GMAT LowEarth_JR.PrimaryBodies = {Earth};
GMAT LowEarth_JR.PointMasses = {Sun, Luna, Jupiter};
GMAT LowEarth_JR.Drag = JacchiaRoberts;
GMAT LowEarth_JR.SRP = Off;
GMAT LowEarth_JR.ErrorControl = RSSStep;
GMAT LowEarth_JR.GravityField.Earth.Degree = 20;
GMAT LowEarth_JR.GravityField.Earth.Order = 20;
GMAT LowEarth_JR.GravityField.Earth.PotentialFile = 'JGM2.cof';
GMAT LowEarth_JR.Drag.AtmosphereModel = 'JacchiaRoberts';
GMAT LowEarth_JR.Drag.F107 = 150;
GMAT LowEarth_JR.Drag.F107A = 150;
GMAT LowEarth_JR.Drag.MagneticIndex = 3;

Create ForceModel HighEarth;
GMAT HighEarth.CentralBody = Earth;
GMAT HighEarth.PrimaryBodies = {Earth};
GMAT HighEarth.PointMasses = {Sun, Luna, Jupiter};
GMAT HighEarth.Drag = None;
GMAT HighEarth.SRP = On;
GMAT HighEarth.ErrorControl = RSSStep;
GMAT HighEarth.GravityField.Earth.Degree = 20;
GMAT HighEarth.GravityField.Earth.Order = 20;
GMAT HighEarth.GravityField.Earth.PotentialFile = 'JGM2.cof';
GMAT HighEarth.SRP.Flux = 1367;
GMAT HighEarth.SRP.Nominal_Sun = 149597870.691;


%----------------------------------------
%---------- Propagators
%----------------------------------------

% Create Propagator
Create Propagator RKV_LowEarth_JR;
GMAT RKV_LowEarth_JR.FM = LowEarth_JR;
GMAT RKV_LowEarth_JR.Type = RungeKutta89;
GMAT RKV_LowEarth_JR.InitialStepSize = 60;
GMAT RKV_LowEarth_JR.Accuracy = 9.999999999999999e-012;
GMAT RKV_LowEarth_JR.MinStep = 0.01;
GMAT RKV_LowEarth_JR.MaxStep = 900;
GMAT RKV_LowEarth_JR.MaxStepAttempts = 50;
GMAT RKV_LowEarth_JR.StopIfAccuracyIsViolated = true;

% Create Propagator
Create Propagator RKF_HighEarth;
GMAT RKF_HighEarth.FM = HighEarth;
GMAT RKF_HighEarth.Type = RungeKutta56;
GMAT RKF_HighEarth.InitialStepSize = 60;
GMAT RKF_HighEarth.Accuracy = 9.999999999999999e-012;
GMAT RKF_HighEarth.MinStep = 1;
GMAT RKF_HighEarth.MaxStep = 900;
GMAT RKF_HighEarth.MaxStepAttempts = 50;
GMAT RKF_HighEarth.StopIfAccuracyIsViolated = true;

%----------------------------------------
%---------- Subscribers
%----------------------------------------

%  Create plots
Create GroundTrackPlot GroundTrackPlot1;
GMAT GroundTrackPlot1.SolverIterations = Current;
GMAT GroundTrackPlot1.Add = {MMS1, MMS2, Sat3};
GMAT GroundTrackPlot1.DataCollectFrequency = 1;
GMAT GroundTrackPlot1.UpdatePlotFrequency = 50;
GMAT GroundTrackPlot1.NumPointsToRedraw = 0;
GMAT GroundTrackPlot1.ShowPlot = true;
GMAT GroundTrackPlot1.CentralBody = Earth;
GMAT GroundTrackPlot1.ShowFootPrints = None;

Create GroundTrackPlot GroundTrackPlot2;
GMAT GroundTrackPlot2.SolverIterations = Current;
GMAT GroundTrackPlot2.Add = {MMS1, GroundStation1};
GMAT GroundTrackPlot2.DataCollectFrequency = 1;
GMAT GroundTrackPlot2.UpdatePlotFrequency = 50;
GMAT GroundTrackPlot2.NumPointsToRedraw = 0;
GMAT GroundTrackPlot2.ShowPlot = true;
GMAT GroundTrackPlot2.CentralBody = Earth;
GMAT GroundTrackPlot2.ShowFootPrints = All;

Create XYPlot XYPlot1;
GMAT XYPlot1.SolverIterations = Current;
GMAT XYPlot1.XVariable = MMS1.A1ModJulian;
GMAT XYPlot1.YVariables = {MMS1.EarthMJ2000Eq.X};
GMAT XYPlot1.ShowGrid = true;
GMAT XYPlot1.ShowPlot = true;

Create OrbitView OrbitView1;
GMAT OrbitView1.SolverIterations = Current;
GMAT OrbitView1.Add = {MMS1, MMS2, Sat3, Earth};
GMAT OrbitView1.CoordinateSystem = EarthMJ2000Eq;
GMAT OrbitView1.DrawObject = [ true true true true ];
GMAT OrbitView1.OrbitColor = [ 255 4227072 65535 32768 ];
GMAT OrbitView1.TargetColor = [ 8421440 4227327 4227327 0 ];
GMAT OrbitView1.DataCollectFrequency = 1;
GMAT OrbitView1.UpdatePlotFrequency = 50;
GMAT OrbitView1.NumPointsToRedraw = 0;
GMAT OrbitView1.ShowPlot = true;
GMAT OrbitView1.ViewPointReference = Earth;
GMAT OrbitView1.ViewPointVector = [ 30000 0 0 ];
GMAT OrbitView1.ViewDirection = Earth;
GMAT OrbitView1.ViewScaleFactor = 2;
GMAT OrbitView1.ViewUpCoordinateSystem = EarthMJ2000Eq;
GMAT OrbitView1.ViewUpAxis = Z;
GMAT OrbitView1.CelestialPlane = Off;
GMAT OrbitView1.XYPlane = On;
GMAT OrbitView1.WireFrame = Off;
GMAT OrbitView1.Axes = On;
GMAT OrbitView1.Grid = Off;
GMAT OrbitView1.SunLine = Off;
GMAT OrbitView1.UseInitialView = On;
GMAT OrbitView1.StarCount = 7000;
GMAT OrbitView1.EnableStars = On;
GMAT OrbitView1.EnableConstellations = On;


%----------------------------------------
%---------- Mission Sequence
%----------------------------------------

BeginMissionSequence;

%  Propagation Sequence One
Propagate Synchronized RKV_LowEarth_JR(Sat3) RKF_HighEarth(MMS) {Sat3.Periapsis, MMS1.TA = 155};

%  Propagation Sequence Two
Propagate Synchronized RKV_LowEarth_JR(Sat3) RKF_HighEarth(MMS) {MMS1.Apoapsis};

%  Add Sat3 to MMS Formation
GMAT MMS.Add = Sat3;

%  Propagation Sequence Three
Propagate RKF_HighEarth(MMS) {Sat3.Periapsis};

% Now pull apart the formation and handle the spacecraft separately
GMAT MMS.Remove = MMS1;
GMAT MMS.Remove = MMS2;
GMAT MMS.Remove = Sat3;

%  Propagation Sequence Four
Propagate Synchronized RKV_LowEarth_JR(MMS1, MMS2, Sat3) {Sat3.Periapsis};




