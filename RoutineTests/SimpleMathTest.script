      GMAT theGroundStation.Latitude = lat;
      GMAT theGroundStation.Altitude = height;
      GMAT X = -(DefaultSC.gsTopo.X);
      GMAT Y = -DefaultSC.gsTopo.Y;
      GMAT Z = -DefaultSC.gsTopo.Z;
   EndFor;
EndFor;
Report rf X Y Z;

GMAT rf.WriteHeaders = Off;
msg = ' ';

