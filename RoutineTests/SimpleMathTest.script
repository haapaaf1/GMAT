BeginMissionSequence;

mu = 398600.4415;


BeginScript
   GMAT var1 = 1.111;
   GMAT var2 = 2.222;
   GMAT var3 = 3.333;
EndScript

GMAT var4 = var2 / var1;
Report rf var1 var2 var3 var4;

For lat = -88:5:88;
   For long = 0:10:360;
