Create String MyVar2
Create Variable MyVar2

%  Test to see if an array name that already exists is refuses
Create Array MyVar3[2,2]
Create Variable MyVar3

%  Testing the expression
Create Variable testVar;
Create Variable testVar2;
Create Variable 1MyVar;
Create Array testArray[3, 4];

GMAT testVar.Expression = DoesNotExist;
GMAT testVar.Expression = 3.5;
GMAT testVar.Expression = testString;
GMAT testVar.Expression = testArray;

GMAT testVar.Expression = DoesNotExist + 2.0;
GMAT testVar.Expression = DoesNotExist + testString;
GMAT testVar.Expression = DoesNotExist + testArray;

GMAT testVar2.Expression = 3.52;
