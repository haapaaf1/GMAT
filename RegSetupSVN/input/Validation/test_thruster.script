GMAT Thruster1.Element1 = 1.01;
GMAT Thruster1.Element1 = 1;
GMAT Thruster1.Element1 = testVar;
GMAT Thruster1.Element1 = testString;
GMAT Thruster1.Element1 = testArray(1,1);

% -----------
% Y-Direction
