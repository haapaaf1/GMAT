% ------------------------------------
% Test validation scripts for Thruster
% ------------------------------------
Create Thruster Thruster1;

Create Variable testVar;
GMAT testVar.Expression = -3.8;
Create String testString;
GMAT testString.Expression = TEST;
Create Array testArray[2, 2];
GMAT testArray.Expression = testArray;
GMAT testArray(1, 1) = 4;

% -----------------
% Unknown parameter
% -----------------
GMAT Thruster1.DoesNotExist = 1;

% -----------------
% Coordinate System
% -----------------
Create CoordinateSystem MyCoordSys;
GMAT Thruster1.CoordinateSystem = DoesNotExist;
GMAT Thruster1.CoordinateSystem = MJ2000EarthEq;
GMAT Thruster1.CoordinateSystem = MyCoordSys;
GMAT Thruster1.CoordinateSystem = 1;
GMAT Thruster1.CoordinateSystem = 1.0;
GMAT Thruster1.CoordinateSystem = testVar;
GMAT Thruster1.CoordinateSystem = testString;
GMAT Thruster1.CoordinateSystem = testArray(1,1);

% ----
% Axis
% ----
GMAT Thruster1.Axis = DoesNotExist;
GMAT Thruster1.Axis = VNB;
GMAT Thruster1.Axis = 1;
GMAT Thruster1.Axis = testVar;
GMAT Thruster1.Axis = testString;
GMAT Thruster1.Axis = testArray(1,1);

% ------
% Origin
% ------
GMAT Thruster1.Origin = DoesNotExist;
GMAT Thruster1.Origin = Earth;
GMAT Thruster1.Origin = 1;
GMAT Thruster1.Origin = 1.0;
GMAT Thruster1.Origin = testVar;
GMAT Thruster1.Origin = testString;
GMAT Thruster1.Origin = testArray(1,1);

% -----------
% X-Direction
% -----------
